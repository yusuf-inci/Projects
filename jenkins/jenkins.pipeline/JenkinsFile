pipeline{
    
    // agent any
    agent {
        docker { image 'maven:3.3.3'}
        //docker { image 'node:16.13.1-alpine' }
    }
    /*tools {
    maven "MAVEN3"
    jdk "OracleJDK8"
    }
    */

    stages {
        stage ('Fetch code') {
            steps {
                git branch: 'paac', url: 'https://github.com/devopshydclub/vprofile-project.git'
            }
        }

        stage ('Build') {
            steps {
                sh 'mvn install'
            }
        }

        stage ('Test') {
            steps {
                sh 'mvn test'
            }
        }
    }
}










